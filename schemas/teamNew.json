{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "required": [
        "code",
        "teamName",
        "isActive",
        "establishedDate"
    ],
    "additionalProperties": false,
    "properties": {
        "code": {
            "type": "string",
            "maxLength": 4
        },
        "teamName": {
            "type": "string",
            "maxLength": 20
        },
        "isActive": {
            "type": "boolean"
        },
        "establishedDate": {
            "type": "string",
            "format": "date"
        },
        "logo": {
            "type": ["string", "null"]
        },
        "captain": {
            "type": ["string", "null"]
        }
    },
    "examples": [
        {
            "code": "MEL",
            "teamName": "Meliora",
            "isActive": true,
            "establishedDate": "2024-08-01",
            "logo": "picture.jpg",
            "captain": "r3th"
        }
    ]
}